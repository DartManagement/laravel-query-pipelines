{
    "name": "dart-management/laravel-query-pipelines",
    "require": {
        "php": "^8.3.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.5",
        "pestphp/pest-plugin-laravel": "^3.0",
        "laravel/pint": "^1.18",
        "larastan/larastan": "^3.0",
        "orchestra/testbench": "^9.6"
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "DartManagement\\LaravelQueryPipelines\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Dart Management",
            "email": "github@dart-management.de"
        }
    ],
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "pint": [
            "./vendor/bin/pint -v"
        ],
        "pint-dirty": [
            "./vendor/bin/pint --dirty"
        ],
        "pint-repair": [
            "./vendor/bin/pint --repair"
        ],
        "pint-test": [
            "./vendor/bin/pint --test"
        ],
        "larastan": [
            "./vendor/bin/phpstan analyse"
        ],
        "test": [
            "php artisan test"
        ],
        "test-mutate": [
            "php artisan test --mutate"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "DartManagement\\LaravelQueryPipelines\\LaravelQueryPipelinesServiceProvider"
            ]
        }
    }
}
